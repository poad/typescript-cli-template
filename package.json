{
  "name": "typescript-cli-template",
  "version": "1.0.0",
  "main": "cli.js",
  "repository": "git@github.com:poad/typescript-cli-template.git",
  "author": "Kenji Saito <ken-yo@mbr.nifty.com>",
  "license": "MIT",
  "type": "module",
  "bin": {
    "typescript-cli-template": "./bin/cli.js"
  },
  "scripts": {
    "test": "NODE_ENV=test jest --coverage --passWithNoTests",
    "build": "swc --delete-dir-on-start src -d dist",
    "dev": "swc --watch src -d dist",
    "lint": "eslint src/**/*.ts",
    "pretest": "pnpm build",
    "package": "echo Skip the packaging process",
    "all": "pnpm lint && pnpm test"
  },
  "devDependencies": {
    "@swc/cli": "^0.3.14",
    "@swc/core": "^1.7.11",
    "@swc/helpers": "^0.5.12",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.15",
    "chokidar": "^3.6.0",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.8.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "regenerator-runtime": "^0.14.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^7.18.0"
  },
  "dependencies": {
    "arg": "^5.0.2"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    }
  },
  "engines": {
    "npm": "use pnpm please!",
    "yarn": "use pnpm please!",
    "pnpm": ">=8.7.1",
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@9.7.1+sha512.faf344af2d6ca65c4c5c8c2224ea77a81a5e8859cbc4e06b1511ddce2f0151512431dd19e6aff31f2c6a8f5f2aced9bd2273e1fed7dd4de1868984059d2c4247"
}
