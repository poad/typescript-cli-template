{
  "name": "typescript-cli-template",
  "version": "1.0.0",
  "main": "cli.js",
  "repository": "git@github.com:poad/typescript-cli-template.git",
  "author": "Kenji Saito <ken-yo@mbr.nifty.com>",
  "license": "MIT",
  "type": "module",
  "bin": {
    "typescript-cli-template": "./bin/cli.js"
  },
  "scripts": {
    "test": "NODE_ENV=test jest --coverage --passWithNoTests",
    "build": "swc --delete-dir-on-start src -d dist",
    "dev": "swc --watch src -d dist",
    "lint": "eslint src/**/*.ts",
    "pretest": "pnpm build",
    "package": "echo Skip the packaging process",
    "all": "pnpm lint && pnpm test"
  },
  "devDependencies": {
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.5.5",
    "@swc/helpers": "^0.5.11",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.11",
    "chokidar": "^3.6.0",
    "eslint": "^9.2.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.5.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "regenerator-runtime": "^0.14.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0"
  },
  "dependencies": {
    "arg": "^5.0.2"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    }
  },
  "engines": {
    "npm": "use pnpm please!",
    "yarn": "use pnpm please!",
    "pnpm": ">=8.7.1",
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@9.1.0+sha512.67f5879916a9293e5cf059c23853d571beaf4f753c707f40cb22bed5fb1578c6aad3b6c4107ccb3ba0b35be003eb621a16471ac836c87beb53f9d54bb4612724"
}
