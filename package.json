{
  "name": "typescript-cli-template",
  "version": "1.0.0",
  "main": "cli.js",
  "repository": "git@github.com:poad/typescript-cli-template.git",
  "author": "Kenji Saito <ken-yo@mbr.nifty.com>",
  "license": "MIT",
  "type": "module",
  "bin": {
    "typescript-cli-template": "./bin/cli.js"
  },
  "scripts": {
    "test": "NODE_ENV=test jest --coverage --passWithNoTests",
    "build": "swc --delete-dir-on-start src -d dist",
    "dev": "swc --watch src -d dist",
    "lint": "eslint src/**/*.ts",
    "pretest": "pnpm build",
    "package": "echo Skip the packaging process",
    "all": "pnpm lint && pnpm test"
  },
  "devDependencies": {
    "@swc/cli": "^0.3.14",
    "@swc/core": "^1.7.4",
    "@swc/helpers": "^0.5.12",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.13",
    "chokidar": "^3.6.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "regenerator-runtime": "^0.14.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^7.18.0"
  },
  "dependencies": {
    "arg": "^5.0.2"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    }
  },
  "engines": {
    "npm": "use pnpm please!",
    "yarn": "use pnpm please!",
    "pnpm": ">=8.7.1",
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@9.6.0+sha512.38dc6fba8dba35b39340b9700112c2fe1e12f10b17134715a4aa98ccf7bb035e76fd981cf0bb384dfa98f8d6af5481c2bef2f4266a24bfa20c34eb7147ce0b5e"
}
