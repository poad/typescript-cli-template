{
  "name": "typescript-cli-template",
  "version": "1.0.0",
  "main": "cli.js",
  "repository": "git@github.com:poad/typescript-cli-template.git",
  "author": "Kenji Saito <ken-yo@mbr.nifty.com>",
  "license": "MIT",
  "type": "module",
  "bin": {
    "typescript-cli-template": "./bin/cli.js"
  },
  "scripts": {
    "test": "NODE_ENV=test jest --coverage --passWithNoTests",
    "build": "swc --delete-dir-on-start src -d dist",
    "dev": "swc --watch src -d dist",
    "lint": "eslint src/**/*.ts",
    "pretest": "pnpm build",
    "package": "echo Skip the packaging process",
    "all": "pnpm lint && pnpm test"
  },
  "devDependencies": {
    "@swc/cli": "^0.3.14",
    "@swc/core": "^1.9.2",
    "@swc/helpers": "^0.5.15",
    "@swc/jest": "^0.2.37",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.6",
    "chokidar": "^3.6.0",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "regenerator-runtime": "^0.14.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^7.18.0"
  },
  "dependencies": {
    "arg": "^5.0.2"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    }
  },
  "engines": {
    "npm": "use pnpm please!",
    "yarn": "use pnpm please!",
    "pnpm": ">=8.7.1",
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@9.12.3+sha512.cce0f9de9c5a7c95bef944169cc5dfe8741abfb145078c0d508b868056848a87c81e626246cb60967cbd7fd29a6c062ef73ff840d96b3c86c40ac92cf4a813ee"
}
