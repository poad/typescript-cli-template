    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.1.1",
    "prettier": "^2.4.0",
    "swc-loader": "^0.1.14",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2",
