{
  "name": "typescript-cli-template",
  "version": "1.0.0",
  "main": "cli.js",
  "repository": "git@github.com:poad/typescript-cli-template.git",
  "author": "Kenji Saito <ken-yo@mbr.nifty.com>",
  "license": "MIT",
  "type": "module",
  "bin": {
    "typescript-cli-template": "./bin/cli.js"
  },
  "scripts": {
    "test": "NODE_ENV=test jest --coverage --passWithNoTests",
    "build": "swc --delete-dir-on-start src -d dist",
    "dev": "swc --watch src -d dist",
    "lint": "eslint src/**/*.ts",
    "pretest": "pnpm build",
    "package": "echo Skip the packaging process",
    "all": "pnpm lint && pnpm test"
  },
  "devDependencies": {
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.5.24",
    "@swc/helpers": "^0.5.11",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.0",
    "chokidar": "^3.6.0",
    "eslint": "^9.4.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.5.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.3.0",
    "regenerator-runtime": "^0.14.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.11.0"
  },
  "dependencies": {
    "arg": "^5.0.2"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    }
  },
  "engines": {
    "npm": "use pnpm please!",
    "yarn": "use pnpm please!",
    "pnpm": ">=8.7.1",
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@9.1.4+sha512.9df9cf27c91715646c7d675d1c9c8e41f6fce88246f1318c1aa6a1ed1aeb3c4f032fcdf4ba63cc69c4fe6d634279176b5358727d8f2cc1e65b65f43ce2f8bfb0"
}
